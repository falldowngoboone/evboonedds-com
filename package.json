{
  "name": "evboonedds-com",
  "version": "0.1.0",
  "main": "index.js",
  "repository": "git@github.com:falldowngoboone/evboonedds-com.git",
  "author": "Ryan Boone <falldowngoboone@gmail.com>",
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "build": "npm-run-all clean build:*",
    "build:css": "sass src/sass:_site/css",
    "build:html": "eleventy --input=src",
    "clean": "rimraf _site",
    "watch": "run-p watch:*",
    "watch:css": "npm run build:css -- --watch",
    "watch:html": "eleventy --serve --input=src",
    "start": "npm run watch"
  },
  "devDependencies": {
    "@11ty/eleventy": "^0.10.0",
    "mustache": "^4.0.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "sass": "^1.26.1"
  }
}
